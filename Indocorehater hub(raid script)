local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/thanhdat4461/OrionMoblie/main/source')))()
local Window = OrionLib:MakeWindow({Name = "INDOCOREHATER HUB(RAIDSCRIPT)", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})
OrionLib:MakeNotification({
    Name = "Hello welcome",
    Content = "Made By ExploitEdge",
    Image = "rbxassetid://4483345998",
    Time = 10
})
local Tab = Window:MakeTab({
	Name = "Brookhaven",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

--[[
Name = <string> - The name of the tab.
Icon = <string> - The icon of the tab.
PremiumOnly = <bool> - Makes the tab accessible to Sirus Premium users only.
]]
Tab:AddLabel("Shout Out To EdgeExploit For Making This Hub And This Script")
Tab:AddButton({
	Name = "Board Changer(Use The Board Before Clicking This Script)",
	Callback = function()
      		---Made By XploitEdge
---Board Changer
---Update 2
---The Wait is Long Because To avoid Tags And Also The Text Changed
---Execute The Script When The Board Item Used
while true do
    wait(4)
    local args = {
        [1] = "Sign",
        [2] = "SignWords",
        [3] = "AFZ,INDOCORE,ODER"
    }
    game:GetService("Players").LocalPlayer.Character.Sign.ToolSound:FireServer(unpack(args))

    wait(4)
    local args = {
        [1] = "Sign",
        [2] = "SignWords",
        [3] = "IS Very Very SUCK And STÃ™PD"
    }
    game:GetService("Players").LocalPlayer.Character.Sign.ToolSound:FireServer(unpack(args))

    wait(4)
    local args = {
        [1] = "Sign",
        [2] = "SignWords",
        [3] = "GETAFATHER#GETBETTERLIFEKID"
    }
    game:GetService("Players").LocalPlayer.Character.Sign.ToolSound:FireServer(unpack(args))

    wait(4)
    local args = {
        [1] = "Sign",
        [2] = "SignWords",
        [3] = "Idc What You Say But Y R So Embarrassing"
    }
    game:GetService("Players").LocalPlayer.Character.Sign.ToolSound:FireServer(unpack(args))
end
  	end    
})

--[[
